{
  "name": "aite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:test": "vue-tsc && vite build --mode development",
    "tsc": "vue-tsc",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue --ignore-path .eslintignore .",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,md,mdx,css,vue}\" --ignore-unknown",
    "pretty-check": "prettier \"./**/*.{js,jsx,ts,tsx,json,md,mdx,css,vue}\" --ignore-unknown --check",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "lucide-vue-next": "^0.370.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.23",
    "vue-router": "4"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.55.1",
    "@types/node": "^20.12.7",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.1",
    "stylelint-config-recommended": "^14.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.9",
    "vue-tsc": "^2.0.13"
  }
